extends base

mixin imageCard(imgSrc)
        div.card
                img.card-image-top(src=imgSrc)
                div.card-body
                        h5.card-title Image

block title
        title Pickit!

block content
        if loggedIn
                a(href="/logout") Log Out
                for image in images
                        +imageCard(image.awsKey)
        else
                a(href="/signup") Sign up
                br
                a(href="/login") Log In

        
